{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "no args",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/cue/main.go",
      "args": [""],
      "env": {},
      "showLog": true,
      "trace": "verbose",
      "dlvLoadConfig": {
        "followPointers": true,
        "maxVariableRecurse": 1,
        "maxStringLen": 800,
        "maxArrayValues": 64,
        "maxStructFields": -1
      }
    },
    {
      "name": "cue help",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/cue/main.go",
      "args": ["help"],
      "cwd": "${workspaceFolder}",
      "env": {},
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "cue vet",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/cue/main.go",
      "args": [
        "vet",
        "--concrete",
        "--schema",
        "#LayersConfig",
        "tmp/layers-config.schema.cue",
        "tmp/layers.data.config.raw.de.json"
      ],
      "cwd": "${workspaceFolder}",
      "env": {},
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "cue def",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/cue/main.go",
      "args": [
        "def",
        "--schema",
        "#LayersConfig",
        "tmp/layers-config.schema.cue",
        "tmp/layers.data.config.raw.de.json"
      ],
      "cwd": "${workspaceFolder}",
      "env": {},
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "cue eval",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/cue/main.go",
      "args": ["eval"],
      "cwd": "${workspaceFolder}",
      "env": {},
      "showLog": true,
      "trace": "verbose"
    }
  ]
}
